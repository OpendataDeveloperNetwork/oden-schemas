      "type": "string",
      "example": "E 8th Ave"
    },
    "CrossStreet": {
      "type": "string",
      "example": "Buchanan Ave"
    },
    "BusStopNumber": {
      "type": "integer",
      "example": 53576
    },
    "Direction": {
      "type": "string",
      "example": "SB"
    },
    "CityName": {
      "type": "string",
      "example": "NEW WESTMINSTER"
    },
    "Accessible": {
      "type": "string",
      "example": "No"
    },
    "Status": {
      "type": "string",
      "example": "ACTIVE"
    }
  }
