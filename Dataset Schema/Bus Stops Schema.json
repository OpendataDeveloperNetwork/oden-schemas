{
  "type": "Feature",
  "geometry": {
    "type": {
      "type": "String",
      "example": "Point",
      "description": "This indicates that the station is geologically a Point.It can be Point, LineString, MultiLineString"
    },
    "coordinates": {
      "items": {
        "type": "float",
        "example": 49.222
      },
